<?xml version='1.0' encoding='utf-8'?>
<widget id="com.notification.bistroux" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BistroUX</name>
    <description>
    BistroUX
  </description>
    <author email="support@cybervationinc.com" href="http://cybervationinc.com">
    Cybervation, Inc.
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">    
        <preference name="android-minSdkVersion" value="20" />
        <preference name="android-maxSdkVersion" value="30" />
        <preference name="android-targetSdkVersion" value="21" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/ldpi.png" />
        <icon density="mdpi" src="res/android/mdpi.png" />
        <icon density="hdpi" src="res/android/hdpi.png" />
        <icon density="xhdpi" src="res/android/xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>    
    <preference name="DisallowOverscroll" value="true" />    
    <preference name="android-windowSoftInputMode" value="adjustResize" />
    <preference name="fullscreen" value="false" />
    <preference name="KeepRunning" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <plugin name="cordova-plugin-device" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-media" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="1.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <variable name="API_KEY_FOR_ANDROID" value="AIzaSyBJZvmtBkzgIFt4YKxn9YaFgFzow9wkTOk" />
    <variable name="API_KEY_FOR_IOS" value="AIzaSyBFxZQaz73onx2qvWPDLq0asFflKiuPb-s" />
    <variable name="PLAY_SERVICES_VERSION" value="11.8.0" />
    
    <plugin name="cordova-plugin-network-information" source="npm" />
    <plugin name="cordova-universal-clipboard" source="npm" />
    <plugin name="cordova-plugin-x-toast" />
    <plugin name="phonegap-plugin-barcodescanner" spec="https://github.com/phonegap/phonegap-plugin-barcodescanner.git">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.1.1" />
    </plugin>
    <engine name="android" spec="~9.0.0" />
    <plugin name="phonegap-plugin-push" spec="~2.2.3">
        <variable name="FCM_VERSION" value="15.0.0" />
        <variable name="SENDER_ID" value="771458932582" />
    </plugin>
    <plugin name="de.appplant.cordova.plugin.printer" />
    <plugin name="Cordova-Plugin-Bluetooth-Printer" spec="https://github.com/CesarBalzer/Cordova-Plugin-BTPrinter.git" />
</widget>
